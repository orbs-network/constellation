            "Config": {
                "genesis-validator-addresses": [
                    "6e2cb55e4cbe97bf5b1e731d51cc2c285d83cbf9",
                    "d27e2e7398e2582f63d0800330010b3e58952ff6",
                    "a328846cd5b4979d68a8c58a9bdfeee657b34de7",
                    "c056dfc0d1fbc7479db11e61d1b0b57612bf7f17"
