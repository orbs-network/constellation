{
  "name": "@orbs-network/orbs-nebula",
  "version": "1.5.4",
  "description": "Nebula is a tool to provision new Orbs Hybrid Blockchain Constellations",
  "main": "./dist/index.js",
  "bin": {
    "nebula": "./dist/bin/nebula.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.tsx -f codeframe",
    "pretest": "npm run typecheck && npm run lint",
    "clean": "rimraf ./dist/ ./exec/",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prepack": "npm run build",
    "compile": "npm run build && pkg . --out-dir ./exec/",
    "prettify": "npx prettier \"src/**/*.ts\" --write",
    "test:unit": "mocha --require ts-node/register test/unit/*",
    "test:e2e": "mocha --no-timeouts --require ts-node/register test/e2e/*",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify"
    }
  },
  "author": "Itamar Arjuan",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.401.0",
    "chalk": "^2.4.2",
    "child-process-promise": "^2.2.1",
    "cli-spinners": "^1.3.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.3.0",
    "ora": "^3.1.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.3.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/child-process-promise": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.24",
    "@types/uuid": "^3.4.7",
    "@types/yargs": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "chai": "^4.2.0",
    "chai-asserttype": "^1.0.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^4.2.0",
    "log-update": "^2.3.0",
    "nock": "^10.0.6",
    "rimraf": "^3.0.0",
    "ts-node": "^8.6.2",
    "typescript": "~3.7.5"
  }
}
